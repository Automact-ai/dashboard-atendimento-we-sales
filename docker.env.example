# DASHBOARD DE ATENDIMENTO - CONFIGURAÇÕES DOCKER
# Copie este arquivo para .env e ajuste as configurações conforme necessário

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001

# Backend Configuration
NODE_ENV=production
PORT=3001

# Security
JWT_SECRET=dashboard-jwt-secret-super-secure-2024
SESSION_SECRET=dashboard-session-secret-super-secure-2024

# Database (SQLite configurações)
DB_PATH=./database/dashboard.db

# Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=5242880

# Logging
LOG_LEVEL=info
LOG_DIR=./logs 